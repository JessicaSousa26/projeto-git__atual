let senhaCorreta = "secreta123";
let senhaDigitada = "";   

do {
    if(senhaDigitada ==="") {
        console.log("Por favor, digite sua senha");
        senhaDigitada = "tentativa1"; // Simula a entrada do usuário
    }   else if(senhaDigitada === "tentativa1"){
            console.log("Tente novamente");
            senhaDigitada = "tentativa2"; // Simula a entrada do usuário
        } else if (senhaDigitada === "tentativa2"){
                console.log("Tente novamente");
                senhaCorreta = "secreta123"; // Simula a entrada do usuário
                senhaDigitada = senhaCorreta
            }   while (senhaDigitada !== senhaCorreta);
                console.log("Senha correta");
        }
